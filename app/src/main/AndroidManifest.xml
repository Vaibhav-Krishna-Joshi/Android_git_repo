<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampusAdobe"
        tools:targetApi="31">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_api_key"/>
        <activity
            android:name=".EditProfile"
            android:exported="false" />
        <activity
            android:name=".MyProperties"
            android:exported="false" />
        <activity
            android:name=".AddProperty"
            android:exported="false" />
        <activity
            android:name=".SignUp"
            android:exported="false" />
        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OwnerPropertyViewActivity"
            android:exported="false"
            android:label="@string/title_activity_owner_property_view"
            android:theme="@style/Theme.CampusAdobe" />
        <activity
            android:name=".TententPropertyViewActivity"
            android:exported="false"
            android:label="@string/title_activity_tentent_property_view"
            android:theme="@style/Theme.CampusAdobe" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.CampusAdobe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>